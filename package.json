{
  "name": "reactor-adobe-va-sample-player",
  "version": "1.0.0",
  "description": "Launch extension which enables video playback using \"HTML5 Video Tag\" and uses \"Adobe Analytics for Video\" for tracking.",
  "author": "",
  "license": "ISC",
  "scripts": {
    "scaffold": "reactor-scaffold",
    "package": "reactor-packager",
    "sandbox": "npm run build && reactor-sandbox",
    "clean": "rimraf dist package-*.zip",
    "lint": "eslint src/**/*.js",
    "build:lib": "babel ./src/lib --out-dir dist/lib --copy-file --presets es2015 --ignore *.test.js --plugins=add-module-exports",
    "build": "npm run clean && npm run lint && npm run build:lib",
    "watch": "watch 'npm run build' ./src",
    "dev": "webpack --watch --env.sandbox",
    "prod": "webpack --env.production"
  },
  "devDependencies": {
    "@adobe/reactor-packager": "^2.6.0",
    "@adobe/reactor-sandbox": "^8.2.1",
    "@adobe/reactor-scaffold": "^1.1.10",
    "@adobe/reactor-turbine": "^22.2.1",
    "babel-cli": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "css-loader": "^0.28.11",
    "eslint": "^4.19.1",
    "eslint-plugin-import": "^2.10.0",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "rimraf": "^2.6.2",
    "style-loader": "^0.20.3",
    "url-loader": "^1.0.1",
    "watch": "^1.0.2",
    "webpack": "^4.5.0",
    "webpack-bundle-analyzer": "^2.11.1",
    "webpack-cli": "^2.0.14",
    "webpack-shell-plugin": "^0.5.0"
  },
  "dependencies": {
    "bootstrap": "^3.0.0",
    "react": "^16.3.1",
    "react-bootstrap": "^0.32.1",
    "react-dom": "^16.3.1",
    "react-redux": "^5.0.7",
    "reduce-reducers": "^0.1.2",
    "redux": "^3.7.2",
    "redux-actions": "^2.3.0",
    "redux-form": "^7.3.0"
  }
}
