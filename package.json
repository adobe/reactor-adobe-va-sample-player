{
  "name": "adobe-va-sample-player",
  "version": "1.0.0",
  "description": "Adobe Analytics for Video Sample Video Player Launch Extension.",
  "author": {
    "name": "adobe"
  },
  "license": "ISC",
  "scripts": {
    "scaffold": "reactor-scaffold",
    "package": "npm run build && reactor-packager",
    "sandbox": "npm run build && reactor-sandbox",
    "clean": "rimraf dist package-*.zip",
    "lint": "eslint src --ext .js --ext .jsx",
    "build:lib": "babel ./src/lib --out-dir dist/lib --copy-file --presets es2015,stage-0 --ignore *.test.js --plugins=add-module-exports",
    "build:view": "webpack --env.production",
    "build": "npm run clean && npm run lint && npm run build:lib && npm run build:view",
    "dev:lib": "watch 'npm run build' ./src/lib",
    "dev:view": "webpack --watch --env.sandbox",
    "analyze:view": "webpack --env.production --env.analyze"
  },
  "devDependencies": {
    "@adobe/reactor-packager": "^3.0.2",
    "@adobe/reactor-sandbox": "^10.0.6",
    "@adobe/reactor-scaffold": "^1.1.10",
    "@adobe/reactor-turbine": "^25.2.1",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "css-loader": "^0.28.11",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "rimraf": "^2.6.2",
    "style-loader": "^0.20.3",
    "uglifyjs-webpack-plugin": "^1.2.5",
    "url-loader": "^1.0.1",
    "watch": "^1.0.2",
    "webpack": "^4.5.0",
    "webpack-bundle-analyzer": "^2.11.1",
    "webpack-cli": "^3.1.1",
    "webpack-shell-plugin": "^0.5.0"
  },
  "dependencies": {
    "bootstrap": "^3.0.0",
    "react": "^16.3.1",
    "react-bootstrap": "^0.32.1",
    "react-dom": "^16.3.1",
    "react-redux": "^5.0.7",
    "reduce-reducers": "^0.4.3",
    "redux": "^3.7.2",
    "redux-actions": "^2.3.0",
    "redux-form": "^7.3.0"
  }
}
